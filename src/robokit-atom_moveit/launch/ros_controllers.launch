<?xml version="1.0"?>
<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find robokit-atom_moveit)/config/ros_controllers.yaml" command="load"/>

  <!-- Load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="left_leg_controller right_leg_controller right_arm_controller left_arm_controller left_hand_controller right_hand_controller legs_controller arms_controller torso_controller whole_body_controller head_controller "/>

</launch>
