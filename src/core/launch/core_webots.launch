<?xml version="1.0"?>
<launch>
  <param name="/use_sim_time" value="true" />
    <!-- start Webots -->
  <arg name="no_gui" default="false," doc="Start Webots with minimal GUI"/>
  <include file="$(find webots_ros)/launch/webots.launch">
    <arg name="mode" value="realtime"/>
    <arg name="no_gui" value="$(arg no_gui)"/>
    <arg name="world" value="$(find core)/webots/worlds/robocup_training.wbt"/>
  </include>

  <node pkg="core" name="camera_info_publisher" type="yaml_to_ros.py" args="$(find core)/config/camera_info.yaml"> </node>
  <node pkg="core" name="core" type="walk_motion_node_dev.py" output="screen"> </node>
  <node pkg="core" name="servo_control" type="servo_control.py" output="screen"> </node>

  
    <remap from="camera_info" to="/robokit/camera/camera_info"/>


</launch>
